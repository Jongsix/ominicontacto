msgstr "**hostname**"

# 2ee91a2fa59745c8afb3bf87de659405
#: ../../install_inventory_file.rst:66
msgid "**ssh port**"
msgstr "**ssh port**"

# 0491836f0ca84ab9b7ec8b16021c9edf
#: ../../install_inventory_file.rst:67
msgid "**ssh user**"
msgstr "**ssh user**"

# 980a141692a54ed4842002a244d6b423
#: ../../install_inventory_file.rst:68
msgid "**dirección IP**"
msgstr "** IP address**"

